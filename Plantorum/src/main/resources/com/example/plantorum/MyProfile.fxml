<AnchorPane fx:id="ScenePane" opacity="0.84" prefHeight="708.0" prefWidth="921.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.plantorum.ProfileController">
